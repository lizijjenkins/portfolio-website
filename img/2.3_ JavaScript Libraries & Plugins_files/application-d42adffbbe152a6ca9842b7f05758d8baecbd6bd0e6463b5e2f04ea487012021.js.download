var dashboardsCourseHome = (function () {
  var JOB_PREP_COURSE = 'job-preparation';

  var init = function () {
    openTabOnPageLoad();
    dashboardTabCourseContent.init();
    dashboardTabCourseProject.init();
    addProgress();
    bindFunctions();
  };

  var bindFunctions = function () {
    $(document).on('click', '[data-call-cta]', callCta);
    $('.call_cancel_modal_1--js').on('click', cancelCall);
    $('.call_cancel_modal_2--js').on('click', openMentorMessage);
  };

  // Bind Methods
  var callCta = function () {
    var cta = $(this).data().callCta;
    if (cta === 'book') {
      window.location.href = `/en/career?course_id=${gon.course_id}`;
    } else if (cta === 'cancel') {
      $('#call_cancel_modal_1').modal('show');
    } else if (cta === 'message') {
      communicationSidebarWrapper.openConversation([gon.current_user.id, gon.mentor_id]);
    } else {
      $zopim.livechat.window.show();
    }
  };

  var cancelCall = function () {
    var call_id = $(this).data('call-id');
    $.ajax(`/en/career/${call_id}/update_status`, {
      method: 'PATCH',
      data: {
        status: 'canceled',
      },
    }).then(function (data) {
      $('.callbox__time').find('p').last().html("<i class='fa fa-exclamation-triangle'></i> Call Cancelled or Missed");
      $('.callbox__footer button').replaceWith("<button class='ds-button__petrol' data-call-cta='book'>Book Again</button>");

      $('#call_cancel_modal_1').modal('hide');
      $('#call_cancel_modal_2').modal('show');
    });
  };

  var openMentorMessage = function () {
    $('.modal').modal('hide');
    communicationSidebarWrapper.openConversation([gon.current_user.id, gon.mentor_id]);
  };

  // Initialize Functions

  var openTabOnPageLoad = function () {
    if (window.location.hash != '') {
      $('a.tab-link[href="' + window.location.hash + '"]').click();
    }
  };

  var addProgress = function () {
    $('.prototype_course_home_week__progress__bar').append(CF.html.loadingSpinner);

    if (gon.course.slug === JOB_PREP_COURSE) {
      _getJobPrepProgress();
    } else {
      _getCourseOrProgramProgress();
    }
  };

  var _getJobPrepProgress = function () {
    $.get('/api/v1/dashboards/job_prep_card', {}, function (response) {
      _progressSuccessResponse(response);
    }).fail(function (error) {
      _progressErrorResponse(error);
    });
  };

  var _getCourseOrProgramProgress = function () {
    $.get('/api/v1/dashboards/courses', {}, function (response) {
      var courses = response.courses.concat(response.courses[0].courses);
      var filteredCourses = courses.filter(x => x !== undefined);
      var card = $.grep(filteredCourses, function (course, i) {
        return course.course_id == gon.course_id;
      })[0];
      _progressSuccessResponse(card);
    }).fail(function (error) {
      _progressErrorResponse(error);
    });
  };

  var _progressSuccessResponse = function (card) {
    var template = JST['views/dashboard/_card_weeks_on_course'](card);
    $('.cf-spinner').hide();
    $('.prototype_course_home_week__progress__bar').append(template);
  };

  var _progressErrorResponse = function (error) {
    errorCard = JST['views/dashboard/error_card'](error);
    $('.cf-spinner').hide();
    $('.prototype_course_home_week__progress__bar').append(errorCard);
  };

  return {
    init: init,
  };
})();
const jobPrep = (function () {
  const thisState = {};
  const init = () => {
    bindFunctions();
  };

  const bindFunctions = () => {
    $(document).on('keyup', '#job_prep_form', toggleSubmitButton);
    $(document).on('click', '#job_prep_form', toggleSubmitButton);
    $(document).on(
      'click',
      '#profile_willing_to_relocate',
      toggleLocationCheckbox
    );
  };

  // Bind functions
  const toggleSubmitButton = function () {
    const city = $('#profile_city');
    const country = $('#job_prep_country_select');
    const relocateCheckbox = $('#profile_willing_to_relocate');
    const relocateLocations = $('#profile_relocate_locations');
    const bio = $('#profile_bio');
    const locationInputCheck = relocateCheckbox.is(':checked')
      ? relocateLocations.val().length > 0
      : true;
    const radioButtonChecked = $('input[type=radio]:checked').size() > 0;

    if (
      city.val().length > 0 &&
      country.val().length > 0 &&
      bio.val().length > 0 &&
      locationInputCheck &&
      radioButtonChecked
    ) {
      dashboardsMain.checkAndToggleNextBtn(
        '#job_prep_optin_form_modal .modal__next',
        true
      );
    } else {
      dashboardsMain.checkAndToggleNextBtn(
        '#job_prep_optin_form_modal .modal__next',
        false
      );
    }
  };

  const toggleLocationCheckbox = () => {
    $('.relocate_asterisk').toggleClass('hidden');
    toggleSubmitButton();
  };

  // Private functions
  const loadFormWithData = () => {
    $.get('/api/v1/me', {}, function (response) {
      const data = response.data.profile;
      const template = JST['views/dashboard/job_prep_optin_form'](data);

      $('#job_prep_optin_form_modal section').append(template);
      buildCountryDropdown(data);
      toggleSubmitButton.bind($('#job_prep_form'))();
      thisState.form = new CF.Form('#job_prep_form');
    });
  };

  const buildCountryDropdown = (data) => {
    const dropdown = document.getElementById('job_prep_country_select');
    const countries = Object.entries(CF.countries.listWithCodes);

    for (const [value, text] of countries) {
      dropdown.append(new Option(text, value, false, value === data.country));
      data.country !== '' &&
        value === data.country &&
        dropdown.parentElement.classList.add('cf-form__group--frozen');
    }
  };

  const submitForm = function () {
    const form = $('#job_prep_form');

    $.ajax({
      data: form.serialize(),
      error: function (data) {
        console.log(data);
      },
      success: function (data) {
        $('#job_prep_confirm_modal').modal('hide');
        dashboardsMain.replaceCtaCard(data);
        if (data) {
          $('#prototype_dashboard_card--csc').addClass('hidden');
          dashboardsMain.renderCscCard(data.state);
        }

        const careerModuleSection = $(
          '.prototype_dashboard__group.prototype_dashboard__group--career_module'
        );
        if (careerModuleSection.find('div').html().length === 0) {
          careerModuleSection.addClass('hidden');
        }
      },
      type: 'POST',
      url: '/api/v1/surveys/generate_job_prep/',
    });
  };

  // Public functions
  const loadOptInFormModal = function () {
    const $modal = $('#job_prep_optin_form_modal');
    if (!$modal.length) {
      const modalTemplate = componentModal.form({
        header: 'Job Preparation Course Opt In',
        modal_id: 'job_prep_optin_form_modal',
        nextBtnAction: 'job_prep_next',
        nextBtnLabel: 'Submit',
        prevBtnAction: 'job_prep_prev',
        prevBtnLabel: 'Back',
        subHeader: 'Please answer a few questions regarding your career needs.',
      });
      $('.prototype_dashboard').append(modalTemplate);
      loadFormWithData();
    }
    dashboardsMain.openModal('#job_prep_optin_form_modal');
  };

  const confirmModal = function () {
    const modalTemplate = componentModal.confirm({
      modal_id: 'job_prep_confirm_modal',
      nextBtnAction: 'job_prep_next',
      nextBtnLabel: 'Confirm',
      preText: 'You have enrolled into the Job Preparation Course.',
      prevBtnAction: 'job_prep_prev',
      prevBtnLabel: 'Back',
      text: 'You now have access to the Course and the Career Support Center, and will soon be assigned a Career Specialist.',
    });

    const $modal = $('#job_prep_confirm_modal');
    if (!$modal.length) {
      $('.prototype_dashboard').append(modalTemplate);
    } else {
      $modal.replaceWith(modalTemplate);
    }
    dashboardsMain.openModal('#job_prep_confirm_modal');
  };

  const jobPrepSlides = [loadOptInFormModal, confirmModal, submitForm];

  const determineModalToShow = () => {
    const slideNumber = dashboardsMain.state.jobPrepSlide || 0;
    return jobPrepSlides[slideNumber]();
  };

  return {
    confirmModal,
    determineModalToShow,
    init,
  };
})();
var dashboardsMain = (function(){
  var thisState = {
    jobPrepSlide: 0,
    specializationSlide: 0,
    bonusSpecializationSlide: 0,
    callsDone: {
      courseCall: false,
      jobPrepCall: false,
      optionalCall: false,
      orientationCall: false,
    }
  };

  var cscActiveSubtitle = "\n  <p class=\"cf-small\">\n  Here you'll find a variety of resources to help you succeed in your job search. <br/>You can use these resources alongside your Job Preparation course and after you finish your studies.  \n  </p>\n\n";
  var cscBlockedSubtitle = "\n  <p class=\"cf-small\">\n  Here you'll find a variety of resources to help you succeed in your job search. <br/><b>They will be unlocked when you opt into the Job Preparation course.</b>  \n  </p>\n\n";

  var cscTeaser = {
    course_id: "csc",
    cardClass: "prototype_dashboard_card--disabled",
    display_id: "prototype_dashboard_card--career-support",
    icon: "https://s3.amazonaws.com/coach-courses-us/public/icons/dashboard/career-support-icon.png",
    name: "Career Support Center",
    state: "read_only",
    type: "course_card",
    is_csc_teaser: true,
    subtitle: cscBlockedSubtitle,
  }

  var init = function(){
    getAndInsertOrientation();
    getAndInsertCourses();
    getAndInsertJobPrep();
    getAndInsertOptional();
    bindFunction();
    specializationCourses.init();
    jobPrep.init();
  };

  var bindFunction = function(){
    $(document).on('shown.bs.modal', '.modal', addModalOpenClass);
    $(document).on('click', '[data-btn-action]', ctaAction);
  };

  var ctaAction = function() {
    if(!$(this).hasClass('disabled')){ // avoide actions of disabled buttons
      var target = $(this).attr('data-btn-action');
      // If Specialization
      if (target === 'specialization') {
        prepareCtaCardToBeReplaced(this);
        specializationCourses.determineSpecializationModalToShow();
      } else if (target === 'specialization_back') {
        (thisState.specializationSlide != 0) && thisState.specializationSlide--;
        specializationCourses.determineSpecializationModalToShow();
      } else if (target === 'specialization_next') {
        thisState.specializationSlide++;
        specializationCourses.determineSpecializationModalToShow();
      // If Bonus Specialization
      } else if (target === 'bonus_specialization') {
        prepareCtaCardToBeReplaced(this);
        specializationCourses.determineBonusSpecializationModalToShow();
      } else if (target === 'bonus_specialization_back') {
        (thisState.bonusSpecializationSlide != 0) && thisState.bonusSpecializationSlide--;
        specializationCourses.determineBonusSpecializationModalToShow();
      } else if (target === 'bonus_specialization_next') {
        thisState.bonusSpecializationSlide++;
        specializationCourses.determineBonusSpecializationModalToShow();
      // If Job Prep
      } else if (target === 'job_prep'){
        prepareCtaCardToBeReplaced(this);
        jobPrep.determineModalToShow();
      } else if (target === 'job_prep_next'){
        thisState.jobPrepSlide++;
        jobPrep.determineModalToShow();
      } else if (target === 'job_prep_prev'){
        (thisState.jobPrepSlide != 0) && thisState.jobPrepSlide--;
        jobPrep.determineModalToShow();
      }
    };
  };

  var prepareCtaCardToBeReplaced = function(card){
    _setSelectedProgram(card);
    // Prepare parent card to be replaced if student chooses a course:
    $('.prototype_dashboard__card--new--js').removeClass('prototype_dashboard__card--new--js');
    $(card).parent('.prototype_dashboard__card--cta').addClass('prototype_dashboard__card--new--js');
  };
  var replaceCtaCard = function(data){
    // Empty CTA card and replace the content with new course card
    $('.prototype_dashboard__card--new--js').html('');
    data && renderAndAppendCard(data, ".prototype_dashboard__card--new--js");
    $('.prototype_dashboard__card--new--js').removeClass();
  }

  var addModalOpenClass = function() {
    // fix for adding 'modal-open' to body to fix scrolling when toggling between bootstrap modals
    $('body').addClass('modal-open');
  };

  // API

  var getAndInsertOrientation = function(){
    $.get('/api/v1/dashboards/orientation_course/', {}, function(response){
      response && renderAndAppendCard(response, ".prototype_dashboard__group--orientation");
    }).done(function() {
      thisState.callsDone.orientationCall = true;
      _checkApiCalls();
    }).fail(function(error){ _renderErrorCard(error) });
  };

  var getAndInsertCourses = function(){
    $.get('/api/v1/dashboards/courses/', {}, function(response){
      response && response.courses.forEach(function(card){
        renderAndAppendCard(card, ".prototype_dashboard__group--course_card");
      });
    }).done(function() {
      thisState.callsDone.courseCall = true;
      _checkApiCalls();
    }).fail(function(error){ _renderErrorCard(error) });
  };

  var getAndInsertJobPrep = function(){
    $.get('/api/v1/dashboards/job_prep_card/', {}, function(response){
      if(response) {
        renderAndAppendCard(response, ".prototype_dashboard__group--career_module");
        renderCscCard(response.csc_active);
      }
    }).done(function() {
      thisState.callsDone.jobPrepCall = true;
      _checkApiCalls();
    }).fail(function(error){ _renderCSCErrorCard(error) });
  };

  var renderCscCard = function(isActive) {
    cscTeaser.state = isActive ? "active" : "blocked";
    cscTeaser.subtitle = isActive ? cscActiveSubtitle : cscBlockedSubtitle;
    renderAndAppendCard(cscTeaser, ".prototype_dashboard__group--career_module");
  }

  var getAndInsertOptional = function(){
    $.get('/api/v1/dashboards/courses_optionals/', {}, function(response){
      response.data && response.data.forEach( function(card){
        renderAndAppendCard(card, ".prototype_dashboard__group--optional");
      });
    }).done(function() {
      thisState.callsDone.optionalCall = true;
      _checkApiCalls();
    }).fail(function(error){ _renderErrorCard(error) });
  };

  // Private Methods

  var _setSelectedProgram = function(card){
    var $parentWithProgramAttendee = $(card).closest('[data-program-attendee-id]');
    var $parentWithProgram = $(card).closest('[data-program-id]');

    if($parentWithProgramAttendee.length > 0) thisState.selectedProgramAttendeeId = $parentWithProgramAttendee.data('program-attendee-id');
    if($parentWithProgram.length > 0) {
      thisState.selectedProgramId = $parentWithProgramAttendee.data('program-id');
      thisState.selectedProgramName = $parentWithProgramAttendee.data('program-name');
    }
  };

  var _renderErrorCard = function(error) {
    errorCard = JST['views/dashboard/error_card'](error);
    $('.prototype_dashboard__card--loading').remove();
    $('.prototype_dashboard__group--optional').append(errorCard);
  };

  var _renderCSCErrorCard = function(error) {
    errorCard = JST['views/dashboard/error_card'](error);
    $('.prototype_dashboard__card--loading').remove();
    $('.prototype_dashboard__career_module').append(errorCard);
  };

  var _renderCourseDetails = function(card){
    card.cardClass = (function(state){
      return `prototype_dashboard_card--${state}`
    })(card.state);
    card.display_id = `prototype_dashboard_card--${card.course_id}`
    card.renderGraduationTime = (card.state == "graduated") ? JST['views/dashboard/_card_graduation_time'](card) : '';
    card.renderSubtitle = JST['views/dashboard/_card_subtitle'](card);
    card.renderDescription = _renderCourseDescription(card);
    card.renderWeeksOnCourse = JST['views/dashboard/_card_weeks_on_course'](card);
    card.renderCourseProgress = _renderProgressBars(card);
    card.renderCourseButtons = card.is_csc_teaser ? JST['views/dashboard/_csc_card_course_buttons'](card) : JST['views/dashboard/_card_course_buttons'](card);
    card.renderIcon = (card.state === 'blocked') ? card.icon.replace(".png", "--disabled.png") : card.icon;
    return card;
  };

  var _renderCourseDescription = function(card){
    if(card.state == 'waiting'){
      return JST['views/dashboard/_card_waiting_description'](card);
    }else{
      return JST['views/dashboard/_card_description'](card);
    };
  };

  var _renderProgressBars = function(card){
    if(card.course_id == 63 ) return JST['views/dashboard/_card_course_orientation'](card);
    if(card.state == 'read_only') return JST['views/dashboard/_card_course_no_progress'](card);
    if(card.state == 'waiting') return '';

    return JST['views/dashboard/_card_course_progress'](card);
  };

  var _renderProgramDetails = function(card){
    card.courses = card.courses.map(function(card){
      // Apply course card details
      return (card.type === 'course_card') ? _renderCourseDetails(card) : card;
    });
    card.progressClass = 'program_progress__bar--' + card.state;
    card.renderProgress = JST["views/dashboard/_card_program_progress"](card);
    card.renderCourses = card.courses.map(function(course){
      return JST["views/dashboard/" + course.type](course);
    }).join('');

    return card;
  };

  var _checkApiCalls = function() {
    // check if all API calls are done
    if (Object.keys(thisState.callsDone).map(function(k){ return thisState.callsDone[k] }).indexOf(false) < 0 ) {
      $('.prototype_dashboard__card--loading').remove();
      _checkCardStatus();
      _checkCSCModuleCardStatus();
    }
  };

  var _checkCardStatus = function() {
    $courseCard = $('.prototype_dashboard__group--course_card').has('div');
    $optionalCard = $('.prototype_dashboard__group--optional').has('div');

    if ($optionalCard.length > 0) {
      $('p.prototype_dashboard__group__label.label__optional').removeClass('hidden');
      setTimeout(function() {
        scrollToJobPrep();
      }, 0);
    } else if ($courseCard.length == 0 && $optionalCard.length == 0) {
      $('.empty_state').removeClass('hidden');
    };
  };

  var _checkCSCModuleCardStatus = function() {
    $courseModuleCard = $('.prototype_dashboard__group--career_module').has('div');

    if ($courseModuleCard.length > 0) {
      $('p.prototype_dashboard__group__label.label__career_module').removeClass('hidden');
    };
  };

  const scrollToJobPrep = () => {
    if (window.location.href.includes('optional_courses')) {
      document.querySelector('#optional_courses').scrollIntoView({
        behavior: 'smooth',
        block: 'start',
      });
    }
  };

  // Public Methods

  var renderAndAppendCard = function(data, appendTo){
    if(data.type == "course_card"){
      data = _renderCourseDetails(data);
    }else if(data.type == "program_card"){
      thisState.selectedProgramId = data.program_id; // set the latest program as default program id
      data = _renderProgramDetails(data);
    }

    var template = JST["views/dashboard/" + data.type](data);
    $(appendTo).append(template);
  };

  var checkAndToggleNextBtn = function(btn, isEnabled){
    if (isEnabled) {
      $(btn).removeClass('disabled');
    } else {
      $(btn).addClass('disabled');
    };
  };

  var openModal = function(id){
    $('.modal').modal('hide');
    $(id).modal('show');
  };

  return {
    state: thisState,
    init: init,
    renderAndAppendCard: renderAndAppendCard,
    openModal: openModal,
    checkAndToggleNextBtn: checkAndToggleNextBtn,
    replaceCtaCard: replaceCtaCard,
    renderCscCard: renderCscCard
  };
})();
