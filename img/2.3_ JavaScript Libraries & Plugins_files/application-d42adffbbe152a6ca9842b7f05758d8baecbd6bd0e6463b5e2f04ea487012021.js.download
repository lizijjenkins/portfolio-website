      if ($('#modal_question_editbox_text').val().length >= 100 || (localStorage.getItem(keyLocalStorage) && localStorage.getItem(keyLocalStorage).length >= 100) ) {
        $('.evaluatePreviewLink--js').removeClass('disabled');
      } else {
          $('.evaluatePreviewLink--js').addClass('disabled');
      }
    }
  };

  return {
    init: init,
    setCurrentStep: setCurrentStep,
    initializeContentForTextArea: initializeContentForTextArea,
    allowPreview: allowPreview
  };
