let calendarEntriesIndex = (function () {
  //eslint-disable-line
  const init = function () {
    bindFunctions();
    gon.calendar_type_booking ? initBooking() : initAvailability();
    scrollToAvailabilities();
    moveModalToBody();
  };

  // =============
  // GENERAL
  // =============

  var bindFunctions = function () {
    $(document).on(
      'click',
      '.glyphicon-menu-right, .glyphicon-menu-left, .today--js',
      showCalendarLoading
    );
    $(document).on('click', '.update-status', updateExistingBookingStatus);
    $('.calendarConversation--js').on('click', linkToConversation);
    $('.switch-active-course--js').on('change', switchActiveCourse);
    $('.message-mentor--js').on('click', messageMentor);
  };

  // link to conversations
  const addActiveClassToBtn = function (target) {
    $('.side-btn').removeClass('active');
    $('[data-target="' + target + '"]').addClass('active');
  };

  var linkToConversation = function (e) {
    e.preventDefault();

    communicationSidebarWrapper.openConversation([
      $(e.target).data('student-id'),
      $(e.target).data('mentor-id'),
    ]);

    $('.modal').modal('hide');
  };

  var messageMentor = function () {
    const userId = $(this).data('user-id');
    const mentorId = $(this).data('mentor-id');
    communicationSidebarWrapper.openConversation([userId, mentorId]);
  };

  // Initialize Methods

  var scrollToAvailabilities = function () {
    const availabilities = $('.available');

    if (availabilities.length > 0) {
      let currently_highest = availabilities[0];

      for (let i = 1; availabilities.length > i; i++) {
        if (
          $(availabilities[i]).position().top <
          $(currently_highest).position().top
        ) {
          currently_highest = availabilities[i];
        }
      }

      var scroll_distance = $(currently_highest).position().top - 40; // 40px come from the fixed table header
    } else {
      var scroll_distance = 240;
    }

    $('tbody').animate({ scrollTop: scroll_distance }, 1000);
  };

  var moveModalToBody = function () {
    $('.modal').appendTo('body');
  };

  // Binding Callbacks

  var switchActiveCourse = function () {
    $(this).parents('form').submit();
  };

  var showCalendarLoading = function () {
    $('#calendar_loading').show();
  };

  var updateExistingBookingStatus = function () {
    if ($(this).data('status') == 'canceled') {
      var user_sure = confirm('Are you sure you want to cancel this call?');
    } else {
      var user_sure = true;
    }

    if (user_sure) {
      $(this).html('<i class="fa fa-spinner fa-spin" aria-hidden="true"></i>');
      $('.update-status').attr('disabled', 'disabled');
      $('.update-status').addClass('disabled');
      const status = $(this).data('status');
      const id = $(this).data('id');

      $.ajax({
        url: '/career/' + id + '/update_status',
        type: 'PATCH',
        dataType: 'json',
        data: {
          status: status,
        },
        success: function (r) {
          $('.update-status').removeAttr('disabled');
          updateStatusCallback(
            r.user.name,
            r.user.tutor,
            r.conversation.id,
            r.entry.attendee_id,
            r.entry
          );
        },
        error: function (xhr) {
          const errors = $.parseJSON(xhr.responseText).errors;
          displayCalendarError(errors[0]);
        },
      }); // ajax
    } // if cancel
  };

  // Private Methods

  var updateStatusCallback = function (
    user_name,
    tutor,
    conversation_id,
    attendee_id,
    entry
  ) {
    if (gon.calendar_type_booking) {
      window.location.reload();
    } else {
      $('.modal').modal('hide'); // Hide existing modals
      $('.respond-buttons [data-id=' + entry.id + ']').hide(400); // Hide buttons
      $('.update-status').removeAttr('disabled'); // Reactivate all other buttons
      $('.update-status').removeClass('disabled'); // Reactivate all other buttons

      $('#cancel_' + entry.id).show(); // Show cancel button in details modal
      // Replace HTML in Message modal:
      // $("#message_recipient_id").html(attendee_id); // Just for debugging
      // Adjust content according to status:
      $('#message_status').html(entry.status);
      // Change the ics download link
      if (entry.status != 'canceled') {
        $('#message_ics_download').html(
          "<a href='/career/" +
            entry.id +
            "/download_ics'><i class='fa fa-download'></i>&nbspDownload .iCal</a>"
        );
        $('#google_calendar_link').html(
          "<a href='/career/" +
            entry.id +
            "/google_calendar_link' target='_blank'><i class='fa fa-calendar-o''></i>&nbspAdd to Google Calendar</a>"
        );
      }

      // reset Calendar:
      if (entry.status == 'canceled') {
        const canceled_times = $('.entry_' + entry.id);
        canceled_times.addClass('available');
        canceled_times.html('');
        canceled_times.removeClass('booking entry_' + entry.id);
      }

      // Show confirmation modal
      $('#message_modal').modal('show');
    }
  };

  var displayCalendarError = function (error_message) {
