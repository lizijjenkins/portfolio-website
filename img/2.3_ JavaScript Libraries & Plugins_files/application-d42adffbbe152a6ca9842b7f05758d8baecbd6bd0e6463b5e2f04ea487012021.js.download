              .removeClass('hover_hour available')
              .attr('class', function (i, c) {
                return c.replace(/(^|\s)available_\d+/, '');
              });
          });
          $('[data-entry]').each(function () {
            $(this).remove();
          });
          updateCopyDropdown();
        },
      }); // ajax
    } else {
      alert(
        'There need to be entries in the week you are currently viewing in order to delete anything.'
      );
    }
  };

  var copyAvailabilities = function (e) {
    e.preventDefault();
    $('#calendar_loading').show();
    // Get all 'data-full-entry' values of the currently visible entries
    const entries = $('.delete-entry')
      .map(function () {
        return $(this).data('full-entry');
      })
      .get();
    const number = $(this).data('amount');
    console.log(number);

    $.ajax({
      url: '/career/copy_week',
      type: 'POST',
      data: {
        entries: entries,
        number: number,
      },
      success: function (r) {
        sessionStorage.no_future_availabilities = 'false'; // for notification (see below)
        $('#calendar_loading').hide();
        console.log(r);
        console.log('Success! ^_^');
      },
      error: function (xhr) {
        const errors = $.parseJSON(xhr.responseText).errors;
        displayCalendarError(errors[0]);
      },
    });
  };

  // Initialize Methods

  var showWarningOnTouchDevices = function () {
    function is_touch_device() {
      return 'ontouchstart' in window || navigator.maxTouchPoints;
    }
    if (is_touch_device()) {
      $('.simple-calendar').prepend(
        "<div class='alert alert-warning'>Please use a non-touch device for reliable scheduling.</div>"
      );
    }
  };

  var checkFutureAvailability = function () {
