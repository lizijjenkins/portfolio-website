    loadSupport();
  };

  const bindFunctions = function () {
    $('#communication').on('click', '#support .faq_question', faqQuestionsTab);
  };

  var faqQuestionsTab = function () {
    if ($(window).width() <= 767) {
      $('.faq_question').removeClass('active');
      $('.faq_question').not(this).next('.faq_answer').slideUp();
      $('.faq_question').not(this).find('.fa-caret-down').removeClass('rotate');
      $(this).next('.faq_answer').slideToggle();
      $(this).find('.fa-caret-down').toggleClass('rotate');
    } else {
      $('.faq_question').removeClass('active');
      $(this).addClass('active');
      $('#conversation .sidebar-content').html(Template.loading_spinner);
      $('#conversation .sidebar-content').html('' +
        '<h4 class="faq_page_question">' + $(this).text() + '</h4>' +
        '<div class="faq_page_answer">' +
          $(this).next('.faq_answer').html() +
        '</div>',
      );
    }
  };

  // Load support
  const loadSupport = function (callback) {
    $('#support .sidebar-content').html(Template.loading_spinner);
    abortAjaxAndGet('/communications/faq', function (data) {
      $('#support .sidebar-content').html(data);
      if (typeof callback === 'function') {
        callback();
      }
    });
  };
  let active_ajax_request = false;
  const abortAjaxAndGet = function (path, callback) {
    active_ajax_request && active_ajax_request.abort();
    active_ajax_request = $.ajax({
      url: path,
      success: function (data) {
        if (typeof callback === 'function') {
          callback(data);
        }
      },
    });
  };

  return {
    init: init,
  };
})();
