    $(document).on('keyup', '#modal_question_editbox_text', textBoxValidation);
    $(document).on('click', '.sample_submission_preview--js', clickCheckBoxPreviewSubmission);
    $(document).on('click', '[data-toggle="notice"]', CareerFoundyCoach.showWarningNotice);
    $(document).on('change', '#vid_review_upload', allowPreviewAfterDirectUpload);
  };

  const displayError = function(text){
    $('.vid_review_upload_container .review_error_container p').html(text);
    $('.vid_review_upload_container .review_error_container p').show();
  }

  const allowPreviewAfterDirectUpload = function(e){
    const file = e.target.files[0];
    if (file.type != "video/mp4") {
      displayError("Invalid file format. Please upload a mp4 file")
      return
    }
    if (Math.floor(file.size/1048576) > 150) {
      displayError("Invalid file size. Please upload a mp4 file < 150MB")
      return
    }
    $('.vid_review_upload_container .review_error_container p').hide();
    $('.ghosted').removeClass('visible');
  }

  var keyLocalStorage = 'submission_tutor_feedback';

  var clickCheckBoxPreviewSubmission = function(){
    $('.sample_submission--js').prop('checked', $(this).is(":checked"));
  };

  // submits the form for comment to the submit_review action
  var clickSubmitPreview = function(e) {
    e.preventDefault();
    $('.review_error_container p').hide();
    const submission_id = $("#submit_review_form--js #submission_id")[0].value;
    const video_review_upload_input = $("#vid_review_upload")[0];
    if (video_review_upload_input && video_review_upload_input.files[0]){
      const file = video_review_upload_input.files[0];
      handleUpload(file, 'video_review', submission_id);
    }else{
      $('#submit_review_form--js').submit();
    }
  };

  const formatData = function(data, file){
    const formData = new FormData();
    formData.append('key', data.fields.key);
    formData.append('acl', 'private');
    formData.append('Content-Type', file.type);
    formData.append('Expires', data.fields.Expires);
    formData.append('success_action_Status', data.fields.success_action_status);
    formData.append('policy', data.fields.policy);
    formData.append('x-amz-credential', data.fields['x-amz-credential']);
    formData.append('x-amz-algorithm', data.fields['x-amz-algorithm']);
    formData.append('x-amz-date', data.fields['x-amz-date']);
    formData.append('x-amz-signature', data.fields['x-amz-signature']);
    formData.append('file', file, file.name);
  
    return formData;
  };

  const uploadFormData = function(file, prefix, submission_id){
    return JSON.stringify({
      file_name: file.name,
      file_path: `${prefix}/${submission_id}/`,
      file_size: file.size,
      file_type: file.type,
      prefix: `${prefix}/`
    });
  }

  const videoUploadProgressHandler = function(e) {
    if(e.lengthComputable) {
      let percentComplete = (e.loaded / e.total) * 100;
      $("#video_upload_progress_bar .progress-bar").width(`${percentComplete}%`)
    }
  }

  const s3UploadSuccessHandler = function(data) {
    $("#submit_review_form--js #video_url").val(data.file_data.url);
    $("#submit_review_form--js #video_source").val('AWSS3');
    $('#submit_review_form--js').submit();
  }
  
  const handleUpload = function(file, prefix, submission_id){
    $.ajax({
      url: '/en/video_review_upload_url',
      data: uploadFormData(file, prefix, submission_id),
      method: 'POST',
      dataType: 'json',
      contentType: 'application/json',
      success: function(data){
        $("#video_upload_progress_bar").removeClass("hidden");
        const formData = formatData(data, file);
        let xhr = new XMLHttpRequest();
        xhr.open("POST", data.url, true);
        xhr.upload.onprogress = videoUploadProgressHandler
        xhr.onload = function() {
          if (xhr.readyState === 4) {
            if (xhr.status === 201) {
              s3UploadSuccessHandler(data)
            } else {
              $('.review_error_container p').show();
            }
          }
        };
        xhr.send(formData);
      },
    });
  }

  // defines criteria when pressing the back button
  // hides divs / defines step numbers / resets svg plant images
  var clickBackToCritera = function(e) {
    e.preventDefault();
    var current_step = $(".step_description").data('step-value');
    if(current_step == 3)
    {
      $("#preview").hide();
      $('#feedback_comment').fadeIn('slow');
      $('#tips').fadeIn('slow');
      resetSvgs();

    }else if(current_step == 2)
    {
      $('#feedback').show().fadeOut('slow', function(){
        $(this).empty();
      })
      $('#evaluate_criteria').fadeIn('slow');
    }
    setCurrentStep(current_step - 1);

  };

  // responsible for filling EVALUATION STEP in the rubrics sub history_header
  // also modifies data variables
  var showCurrentStep = function(){
    var stepValue = $('.step_description').data('step-value');
    $('.bar').map(function () {
      if($(this).data().step <= stepValue) {
        $(this).addClass('filled');
      } else {
        $(this).removeClass('filled');
      }
    });
    $('.stepValue--js').html(stepValue);
  };



  // resets the svg plant images in the preview card
  var resetSvgs = function() {
    $('.svg').each(function() {
      $(this).removeClass('fill')
    });
  };

  // adds the selected tip link to the body of the comment form
  var getUrlOfLink = function(e){
    e.preventDefault();
    var linkHref = $(this).parents('a').attr('href');
    var linkTitle = $(this).siblings('span').html();
    var toInsert = "\n"+"[" + linkTitle + "](" + linkHref + ")";
    var textValue = $('.evaluate_body .comment-form textarea').val();
    var textBox = $('.evaluate_body .comment-form textarea');
    textBox.val(textValue + '\n' + toInsert);
    autosize.update($('.evaluate_body .comment-form textarea'));
  };

  // shows the preview card and hides the form and feedback divs
  var showPreviewTemplate = function(e) {
    e.preventDefault();
    if(!$('.evaluatePreviewLink--js').hasClass('disabled')) {
      var submissionStatusElement = $('.submission_status');
      if(submissionStatusElement.data('submision-status') === 'approved'){
          $('.sample_submission').show();
          $('.sample_submission_preview--js').prop('checked', $(".sample_submission--js").is(":checked"));
      }
      $("#preview").fadeIn('slow');
      $('#feedback_comment').hide();
      $('#tips').hide();
      $('.comment_text p').html(markdownConverter($('#modal_question_editbox_text')));
      $('.preview_criteria').html(submissionStatusElement.data('submision-status_verbose'));
      fillSvg(submissionStatusElement.data('submision-status'));
      setCurrentStep(3);
    }
    setTargetOnUrls();
  };

  const setTargetOnUrls = function () {
    $('.comment_text').find('a').each(function () {
      $(this).attr('target', '_blank');
    });
  };

  var showCharactersLeft  = function(value) {

    var character_count = $(returnHtmlFromString(value)).text().length,
        reverse_count = 100 - character_count;
    return (reverse_count > 0) ? ("Write " + reverse_count + " more characters.") : "";
  };

  // validates the length of the comment left by a mentor
  var textBoxValidation = function() {
    var value = $(this).val();
    if(localStorage)
    {
      localStorage.setItem(keyLocalStorage, value);
      $('.counter_comment').html(showCharactersLeft(value));
      validateLength($(returnHtmlFromString(localStorage.getItem(keyLocalStorage))).text(), 100);
    }else{
        validateLength(value, 100);
    }
  }

  var returnHtmlFromString = function(string_content){
    var converter = CareerFoundyCoach.htmlConverter();
        text = string_content
    return converter.makeHtml(text);
  }

  // function to check length within text box
  var validateLength = function(input_value, length) {
    if (input_value.length < length) {
      $('.evaluatePreviewLink--js').addClass('disabled');
    } else {
      $('.evaluatePreviewLink--js').removeClass('disabled')
    }
  }

  // Private Methods
  var fillSvg = function(criteria) {
    $('svg.' + criteria ).addClass('fill')
  };

  var markdownConverter = function(element) {
    var converter = CareerFoundyCoach.htmlConverter();
    return converter.makeHtml(element.val());
  };

