  var bookingHoverStart = function () {
    // Only full hours can be selected
    if ($(this).next().hasClass('available')) {
      $(this).addClass('hover_student');
      $(this).next().addClass('hover_student');
    }
  };

  var bookingHoverStop = function () {
    $(this).removeClass('hover_student');
    $(this).next().removeClass('hover_student');
  };

  var setNewBooking = function () {
    if (
      $(this).next().hasClass('available') &&
      !$(this).next().hasClass('unselected') &&
      !$(this).next().hasClass('booking')
    ) {
      starting = $(this);
      ending = $(this).next();

      start_minute = starting.data('starttime');
      end_minute = ending.data('endtime');
      readable_time = starting.data('readable-time');

      // Show Details modal before booking
      $('.new_time').html(readable_time);
      $('#new_booking').modal();
    } // if 1 hour
  };

  var submitBooking = function () {
    $('#new_booking').modal('hide');
    $('#calendar_loading').show();
    // Make PATCH instead of POST request if a call is canceled or never happened
    if (typeof update_booking_id !== 'undefined') {
      $.ajax({
        url: '/career/' + update_booking_id,
        type: 'PUT',
        data: {
          calendar_entry: {
            start_time: start_minute,
            end_time: end_minute,
            entry_type: 'booking',
            status: 'open',
            mentor_id: gon.mentor.id,
            attendee_id: gon.attendee.id,
          },
        },
        success: function (r) {
          window.location.reload();
        },
        error: function (xhr) {
          $('#calendar_loading').hide();
          const errors = $.parseJSON(xhr.responseText).errors;
          displayCalendarError(errors[0]);
        },
      }); // ajax
    } else {
      $.ajax({
        url: '/career',
        type: 'POST',
        data: {
          calendar_entry: {
            start_time: start_minute,
            end_time: end_minute,
            entry_type: 'booking',
            status: 'open',
            mentor_id: gon.mentor.id,
            attendee_id: gon.attendee.id,
          },
        },
        success: function (r) {
          if (window.location.href.includes('fromNotification=true')) {
            window.location.href =
              window.location + '&bookedFromNotification=true';
          } else {
            window.location.reload();
          }
        },
        error: function (xhr) {
          $('#calendar_loading').hide();
          const errors = $.parseJSON(xhr.responseText).errors;
          displayCalendarError(errors[0]);
        },
      }); // ajax
    } // if typeof
  };

  var interruptBooking = function () {
    $('#new_booking').modal('hide');
  };

  var deleteBooking = function () {
    $('#calendar_loading').show();
    const id = $(this).data('entry');
    $.ajax({
      url: '/career/' + id,
      type: 'DELETE',
      success: function (r) {
        window.location.reload();
      },
    });
  };

  return {
    init: init,
    initSelectables: initSelectables,
    scrollToAvailabilities: scrollToAvailabilities,
    updateCopyDropdown: updateCopyDropdown,
  };
})();
var communicationIndex = (function () { //eslint-disable-line
