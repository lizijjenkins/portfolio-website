const PaymentWithoutContract = (function () {
  init = function () {
    domReady(function () {
      initSelectors();
      initUpdateButton();
    });
  };

  domReady = function (fn) {
    document.onreadystatechange = () => {
      if (document.readyState === 'interactive') { fn(); }
    };
  };

  initSelectors = function () {
    $('.contract_selector').change(function () {
      const selector = this;
      const selectorData = $(selector).data();
      const newContractId = $(selector).val();
      const button = $(`tr#payment-${selectorData.paymentId} button.update_contract`);

      if (newContractId) {
        $(button).removeClass('hidden');
      } else {
        $(button).addClass('hidden');
      }
    });
  };

  initUpdateButton = function () {
    const pwc = this;
    $('button.update_contract').click(function () {
      const button = this;
      const buttonData = $(button).data();
      const actionURL = buttonData.action;
      const paymentId = buttonData.paymentId;
      const contractId = $(`select#payment_${paymentId}_payment_contract_id`).val();
      const token = $('meta[name="csrf-token"]').attr('content');

      const updateData = { actionURL, contractId, paymentId, token };

      const successHandler = function (data) {
        $(`tr#payment-${data.id}`).fadeOut(800, function () {
          $(this).remove();
          if ($('tr[id*=payment]').length === 0) {
            $('table.payments_table thead').addClass('hidden');
            $('tr.empty_table').removeClass('hidden');
          }
        });
      };

      const errorHandler = function (response) {
        $('#payment_without_contract .alert-danger p').text(response.errors);
        $('#payment_without_contract .alert-danger').fadeIn(400).removeClass('hidden');
        setTimeout(function () {
          $('#payment_without_contract .alert-danger').fadeOut(1200, function () {
            $('#payment_without_contract .alert-danger p').text('Something went wrong');
          });
        }, 1200);
      };

      pwc.updatePayment(updateData, successHandler, errorHandler);
    });
  };

  checkStatus = async function (response) {
    if (response.status >= 200 && response.status < 300) { return await response.json(); }
    throw await response.json();
  };

  updatePayment = async function ({ actionURL, token, paymentId, contractId }, successHandler = () => {}, errorHandler = () => {}) {
    return fetch(actionURL, {
      body: JSON.stringify({
        authenticity_token: token,
        contract_id: contractId,
        payment_id: paymentId,
      }),
      headers: { 'Content-Type': 'application/json' },
      method: 'PATCH',
    }).then(checkStatus).then(successHandler).catch(errorHandler);
  };

  return {
    init: init,
  };
})();
