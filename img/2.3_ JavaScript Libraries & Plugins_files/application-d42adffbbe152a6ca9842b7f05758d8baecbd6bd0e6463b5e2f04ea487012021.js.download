    method: 'POST',
    data: {
      event: event,
      data: data,
    },
  });
};

const usersSettings = (function () {
  const referralLink = '';

  const init = function () {
    // in case the user is redirected to the referral settings tag
    $(document).ready(function () {
      const queryParams = new URL(document.location).searchParams;
      const tabParam = queryParams.get('tab');
      const activeTab = window.location.href.split('#').pop();
      const isActiveReferralsTab =
        tabParam === 'user_referrals' || activeTab === 'user_referrals';
      if (isActiveReferralsTab) {
        trackReferralVisit();
      }
    });
    $(document).on('copy', '#certificate-message', trackCopyCertificateMessage);
    $(document).on('click', '.add-certificate', trackAddCertificate);
    $(document).on('click', '#copy_to_clipboard--js', trackCopyClicks);
    $(document).on('click', '#user_referrals_nav', trackReferralVisit);
    $(document).on('click', '#social_share', trackShareClicks);
    $('.copy_to_clipboard--link').bind('copy', trackReferralLinkCopy);
    $(document).on('click', '#magazine_article_link', trackMagazineArticleLink);
    $(document).on('click', '#share_your_link', trackShareYourLink);
    showPayoutData();
    $(document).on('click', '#perk_redeemable_link', getPerkRedeemableLink);
  };

  const trackCopyCertificateMessage = function () {
    trackEvent('certificate_message_copied');
  };

  const trackAddCertificate = function (e) {
    const dataset = e.currentTarget.dataset;
    trackEvent('certificate_add_clicked', {
      course_id: dataset.courseId,
      media: dataset.media,
      program_id: dataset.programId,
    });
  };

  const trackMagazineArticleLink = function () {
    trackEvent('referral_magazine_article_link_click');
  };

  const trackShareYourLink = function () {
    trackEvent('referral_share_your_link_click');
  };

  const trackReferralVisit = function () {
    trackEvent('referral_tab_visit');
  };

  const trackCopyClicks = function () {
    trackEvent('referral_copy_link');
  };

  const trackShareClicks = function (e) {
    const networkName = e.target.dataset['name'];
    trackEvent('referral_share_link', { network: networkName });
  };

  const trackReferralLinkCopy = function (_) {
    if (
      $('.copy_to_clipboard--link').val() === window.getSelection().toString()
    ) {
      trackCopyClicks();
    }
  };

  const showPayoutData = function () {
    const payoutData = document.querySelector('#payout_information');
    const linkElement = document.querySelector('#link_to_payout_form_page');

    if (!linkElement) {
      return;
    }

    fetch('/api/v1/finance/payout_information')
      .then((response) => response.json())
      .then((data) => {
        if (data.contact_name) {
          payoutData.innerHTML = `
            <h5>${data.contact_name}</h3>
            <p>${data.address_line}</p>
            <p>${data.address_second_line}</p>
            <p>${data.city} ${data.postal_code}</p>
            <p>${data.country}</p>
          `;
          linkElement.innerText = 'Edit payout information';
        } else {
          linkElement.innerText = 'Create payout information';
        }
      });
  };

  const getPerkRedeemableLink = function (e) {
    fetch($(this).data('redeemable_link'))
      .then(function (response) {
        if (!response.ok) {
          throw Error(response.statusText);
        }
        return response.json();
      })
      .then((data) => {
        window.open(data.link, '_blank').focus();
      })
      .catch((errors) => {
        alert(
          'We are unable to process this request. Please contact your Student Advisor'
        );
      });
  };

  return {
    init,
  };
})();
