      '/steps/' + gon.step_id + '/recently_approved',
      '#recently-approved'
    );
  };

  // Update URL with hash based on location on page (and update slack modal with hash)
  const updatePageUrl = function (e) {
    hashval = $("a[href^='#']", e.target).attr('href');
    history.replaceState({}, '', hashval);
    $('#comment_task_id').val(hashval);
  };

  /* ------------------ FOCUS MODE ------------------ */
  const initializeFocusMode = function () {
    const fullscreenAvailable =
      document.fullscreenEnabled ||
      document.mozFullScreenEnabled ||
      document.webkitFullscreenEnabled ||
      document.msFullscreenEnabled;
    if (fullscreenAvailable) {
      $('#focus_mode_on_btn').show();
    }
  };

  const requestFullScreen = function (element) {
    const requestMethod =
      element.requestFullScreen ||
      element.webkitRequestFullScreen ||
      element.mozRequestFullScreen ||
      element.msRequestFullScreen;
    if (requestMethod) {
      requestMethod.call(element);
    }
  };

  const exitFullScreen = function (element) {
    // Supports most browsers and their versions.
    const requestMethod =
      element.exitFullscreen ||
      element.webkitExitFullscreen ||
      element.mozCancelFullScreen ||
      element.msExitFullscreen;
    if (requestMethod) {
      requestMethod.call(element);
    }
  };

  const focusModeOn = function () {
    $('body').attr('id', 'focus_mode_on');

    $('#focus_mode_on_btn').hide();
    $('#focus_mode_off_btn').show();
    requestFullScreen(document.documentElement); // Make the body go full screen.
  };

  const focusModeOff = function () {
    $('body').attr('id', '');

    $('#focus_mode_on_btn').show();
    $('#focus_mode_off_btn').hide();
    exitFullScreen(document); // Make the body exit full screen.
  };
  /* ------------------ END FOCUS MODE ------------------ */

  // Private Methods
  const buildSectionLinks = function () {
    // add anchor links to exercise content above h4 tags, excluding modals
    $('#day_contents h4:not(.heading):not(.modal-text)').each(function (index) {
      anchor = getSlug($(this).text());
      $(this).before('<a id="' + anchor + '" class="task-card-link"></a>');
      // add menu links to anchor tags
      if ($(this).text() === 'Task') {
        link =
          '<li><a href="#' +
          anchor +
          '" class="section-link"><div class="task-nav-item-container"><div class="course_menu_dot task"></div><div class="course_menu_text ds-text__body--medium ds-text--bold">' +
          $(this).text() +
          '</div></div></a></li>';
      } else {
        link =
          '<li><a href="#' +
          anchor +
          '" class="section-link"><div class="task-nav-item-container"><div class="course_menu_dot"></div><div class="course_menu_text ds-text__body--medium ds-text--bold">' +
          $(this).text() +
          '</div></div></a></li>';
      }
      $('#section-links').append(link);
    });

    $('.section-link').on('click', () => {
      /**
       * After user clicks on a section link we need to add some space on the top of the window
       * as a title (of a specific part of the content) needs to be in the view and not hidden
       * inside the top nav bar and/or breadcrumb navigation (without this setup the title would
       * be hidden inside the top nav bar and/or breadcrumb navigation).
       *
       * setTimeout calls scrollTo after page's view is updated due to the hash change (i.e.
       * after default browser behaviour).
       */
      setTimeout(() => window.scrollTo(window.scrollX, window.scrollY - 90), 0);
    });
  };

