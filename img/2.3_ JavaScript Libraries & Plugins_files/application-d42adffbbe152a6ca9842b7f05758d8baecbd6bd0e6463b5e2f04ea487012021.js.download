  };

  var bindFunctions = function () {
    $('.course_end_date').on('click', '#end_date_close', dismissPopover);
    $('.course_home_accordion').on('click', toggleAccordionArrow);
    initPopOverScroll();
  };

  var dismissPopover = function () {
    $('.end_date_info').popover('hide');
  };

  var toggleAccordionArrow = function () {
    if ($(this).find('.fa-angle-down').hasClass('open')) {
      $(this).find('.fa-angle-down').removeClass('open');
    } else {
      $(this).find('.fa-angle-down').addClass('open');
    };
  };

  const addActiveClassToBtn = function (target) {
    $('.side-btn').removeClass('active');
    $('[data-target="' + target + '"]').addClass('active');
  };

  var initPopover = function () {
    $('.end_date_info').popover({
      trigger: 'hover',
      placement: 'right',
      container: '.course_end_date',
      html: 'true',
      delay: { hide: 500 },
    });
    $('#progress-popover').popover({
      trigger: 'hover',
      placement: 'left',
      html: 'true',
      delay: { hide: 500 },
    });
    $('.prototype_course_home__calls [data-toggle="popover"]').popover({
      trigger: 'hover',
      placement: 'left',
      html: 'true',
    });
  };

  // fix for issues with popover firing off of window in DOM
  var initPopOverScroll = function () {
    const scrollTop = $(this).scrollTop();
    if (gon.show && scrollTop > 544) bindPopOverScroll();
  };

  // bind popover scroll even only if the popover is off the screen
  // othewrwise it will fire on page load as normal
  var bindPopOverScroll = function () {
    $(window).scroll(function () {
      const scrollTop = $(this).scrollTop();
      if (scrollTop < 544) {
        $('.end_date_info').popover('show');
        $(window).off('scroll');
      }
    });
  };

  var showOnLoad = function () {
    const scrollTop = $(this).scrollTop();
    if (gon.show && scrollTop < 648) $('.end_date_info').popover('show');
    if (window.innerWidth <= 700) $('.end_date_info').siblings('.popover').css({ left: '25px' });
  };

  var circleConfig = function () {
    $.circleProgress.defaults.dashesAmount = 15;
    $.circleProgress.defaults.dashSize = 0.03; // in radians

    $.circleProgress.defaults.drawArc = function (v) {
      const ctx = this.ctx;
      const r = this.radius;
      const t = this.getThickness();
      const c = this.arcCoef;
      const a = this.startAngle + (1 - c) * Math.PI;

      v = Math.max(0, Math.min(1, v));

      ctx.save();
      ctx.beginPath();

      ctx.arc(r, r, r - t / 2, a, a + 2 * c * Math.PI * v);

      ctx.lineWidth = t;
      ctx.lineCap = this.lineCap;
      ctx.strokeStyle = this.arcFill;
      ctx.stroke();
      ctx.restore();

      this.addDashes();
    };

    $.circleProgress.defaults.addDashes = function () {
      const ctx = this.ctx;
      const r = this.radius;
      const t = this.getThickness();
      const d = this.dashSize;
      const c = this.arcCoef;
      const a = this.startAngle + (1 - c) * Math.PI;
      const n = this.dashesAmount;

      ctx.save();
      ctx.lineCap = 'butt';
      ctx.lineWidth = t;
      ctx.strokeStyle = '#fff';

      // not very cross-browser option that makes the "dashes" transparent
      // if it will not work, remove this line - "dashes" will be white
      ctx.globalCompositeOperation = 'destination-out';

      for (let i = 0; i < n; i++) {
        ctx.beginPath();
        const s = a + 2 * c * (1 - i / n) * Math.PI;
        ctx.arc(r, r, r - t / 2, s - d / 2, s + d / 2);
        ctx.stroke();
      }

      ctx.restore();
    };
  };

  var setCircleDefaults = function () {
    $('.full.circle').circleProgress({
      arcCoef: 1,
      dashSize: Math.PI / 40,
      value: gon.submitted_in_week / gon.weekly_goal,
      size: 115,
      startAngle: 4.72,
      thickness: 10.5,
      emptyFill: '#f1f1f1',
      fill: '#5ca2b2',
      animation: false,
      dashesAmount: gon.weekly_goal,
    });
  };

  return {
    init: init,
  };
})();
