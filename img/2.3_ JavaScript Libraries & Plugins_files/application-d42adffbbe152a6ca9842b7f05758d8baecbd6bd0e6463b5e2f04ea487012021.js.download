    // Internal Methods
    bindfunctions();
    loadSidebarMenu();
    loadAllAjaxPanelsOnStep();
    initializeFocusMode();
    lazyLoadIframes();
    removeTopNavBarShadow();
  };

  const lazyLoadIframes = () => {
    const iframes = document.querySelectorAll('iframe[data-src]');
    if (iframes.length > 0) {
      const options = {
        root: null,
        rootMargin: '0px',
        threshold: 0.01,
      };
      const observer = new IntersectionObserver((entries) => {
        entries.forEach((entry) => {
          if (entry.isIntersecting) {
            const iframe = entry.target;
            iframe.src = iframe.dataset.src;
            observer.unobserve(iframe);
          }
        });
      }, options);
      iframes.forEach((iframe) => {
        observer.observe(iframe);
      });
    }
  };

  const bindfunctions = function () {
    $('#focus_mode_on_btn').on('click', focusModeOn);
    $('#focus_mode_off_btn').on('click', focusModeOff);
    $(window).on('activate.bs.scrollspy', updatePageUrl);
  };

  const loadSidebarMenu = function () {
    buildSectionLinks();
    $('body').scrollspy({ offset: 100, target: '#task_nav' });
    $('#task_nav').fadeIn(2000);
    step_id = gon.step_id;
  };
  // Load Exercise Survey via ajax
