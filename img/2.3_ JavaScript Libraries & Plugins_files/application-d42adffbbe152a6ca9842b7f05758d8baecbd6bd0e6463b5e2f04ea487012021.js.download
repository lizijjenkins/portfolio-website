const multiformUploader = (function () {
  const performUpload = function (uploadForm) {
    uploadForm.fileupload({
      add: function (e, data) {
        sending(e);
        $.ajax({
          async: false,
          data: {
            file_name: data.files[0].name,
            file_path: uploadForm.data('remote-provider-key'),
            file_size: data.files[0].size,
            file_type: data.files[0].type,
            prefix: uploadForm.data('remote-provider-key-starts-with'),
          },
          dataType: 'json',
          success: function (retdata) {
            const formData = new FormData();
            Object.entries(retdata.fields).forEach(field => formData.append(field[0], field[1]));
            formData.append('file', data.files[0], data.files[0].name);
            formData.append('Content-Type', retdata.file_data.type);
            const request = new XMLHttpRequest();
            request.open('POST', retdata.url);
            request.send(formData);

            request.onreadystatechange = function () {
              const status = request.status;
              if (status === 0 || (status >= 200 && status < 400)) {
                success(e, uploadForm, retdata.file_data);
              } else {
                failure(e);
              }
            };
          },
          type: 'GET',
          url: uploadForm.data('upload-form-data-url'),
        });
      },
      autoUpload: true,
    });
  };

  return {
    performUpload: performUpload,
  };
})();

const sending = function (e) {
  $('#s3-upload-error').remove();
  target = $(e.target);
  target.find('.btn-file').addClass('disabled');
  target.find('.btn-file .fa').removeClass('fa-upload');
  target.find('.btn-file .fa').addClass('fa-spin fa-spinner');
  target.find('#file').attr('disabled', 'disabled');
};

const success = function (e, uploadForm, fileData) {
  const params = {};
  target = $(e.target);
  target.find('.btn-file').removeClass('disabled');
  target.find('.btn-file .fa').removeClass('fa-spin fa-spinner');
  target.find('.btn-file .fa').addClass('fa-upload');
  target.find('#file').removeAttr('disabled');

  if (uploadForm.data('callback-url')) {
    const attributes = {
      filename: fileData.name,
      filesize: fileData.size,
      filetype: fileData.type,
    };

    attributes[uploadForm.data('callback-param')] = fileData.url;
    Object.assign(attributes, params.additionalData);
    $.ajax({
      data: attributes,
      type: uploadForm.data('callback-method'),
      url: uploadForm.data('callback-url'),
    });
  } else {
    location.reload();
  }

  uploadForm.prop('action', '');
  clearInputs(uploadForm);
};

const clearInputs = (uploadForm) => {
  const inputs = ['key', 'acl', 'Expires', 'success_action_status', 'policy', 'x-amz-credential', 'x-amz-algorithm', 'x-amz-date', 'x-amz-signature'];
  inputs.forEach(input => {
    uploadForm.find(`input[name=${input}]`).val('');
  });
};

const failure = (e) => {
  target = $(e.target);
  target.find('.btn-file').removeClass('disabled');
  target.find('.btn-file .fa').removeClass('fa-spin fa-spinner');
  target.find('.btn-file .fa').addClass('fa-upload');
  target.find('#file').removeAttr('disabled');
  if ($('#s3-upload-error').length === 0) {
    target.append('<p id="s3-upload-error" style="color:tomato"><b>It was not possible to upload your file. Please try again.</b></small></p>');
  };
};
var userNotifications = (function () {
  var init = function () {
    if ($('#notifications').length) $('#notifications').load('/nav_notifications');
    bindFunctions();
  };

  var bindFunctions = function () {
    $(document).on('click', '.clear-notifications--js', clearNotifications);
    $(document).on('click', '.notification-link--js', navigateToLinkDestination);
    $('.filter_unread_notifications').on('click', filterUnreadNotifications);
    trackBookMentor();
  };

  const navigateToLinkDestination = function () {
    const id = $(this).data('id');
    const link = $(this).data('link');
    const type = $(this).data('comment-type');

    dataLayer.push({
      event: 'GAEvent',
      eventAction: 'Clicked Notification Link',
      eventCategory: 'Notifications',
      eventLabel: `Notification type - ${type}`,
      eventValue: '',
    });

    $.post('/clear_notification', { id: id }, () => {
      $(this).removeClass('unseen');
    }).done(() => redirectToLink(link, type));
  };

  const redirectToLink = (link, type) => {
    if (type === 'DiscourseTopic') {
      window.open(link, '_blank').focus();
    }else{
      const section = 'optional_courses';
      if (window.location.href.includes(section) && link.includes(section)) {
        document.querySelector(`#${section}`).scrollIntoView({
          behavior: 'smooth',
          block: 'start',
        });
      } else {
        history.pushState(null, null, link);
        location.reload();
      }
    }
  };

  var clearNotifications = function () {
    $.post('/clear_notifications', {}, function () {
      $('.notification').each(function () {
        $(this).removeClass('unseen');
      });
      $('.alerts-indicator img').attr('src', 'https://images.careerfoundry.com/public/images/notifications-grey.png');
      $('span.notifications-count').remove();
    });
  };

  var filterUnreadNotifications = function () {
    $(this).hide();
    $('label').prepend("<i class='fa fa-circle-o-notch fa-spin'> </i> ");

    if ($(':checkbox:checked').length > 0) {
      window.location.href = '?unseen=1';
    } else {
      window.location.href = '?unseen=0';
    }
  };

  var trackBookMentor = function () {
    var bookMetorSelector = '*[data-comment-type="BookFirstMentorCall"]';
    $(window).on('load', function () {
      if ($(bookMetorSelector)[0]) {
        window.dataLayer.push({
          funnelStage: 'NotificationReceived',
          event: 'BookFirstMentor',
        });
      }

      if (window.location.href.includes('fromNotification=true')) {
        window.dataLayer.push({
          funnelStage: 'NotificationClicked',
          event: 'BookFirstMentor',
        });
      }

      if (window.location.href.includes('bookedFromNotification=true')) {
        window.dataLayer.push({
          funnelStage: 'MentorBooked',
          event: 'BookFirstMentor',
        });
      }
    });
  };

  return {
    init: init,
  };
})();
