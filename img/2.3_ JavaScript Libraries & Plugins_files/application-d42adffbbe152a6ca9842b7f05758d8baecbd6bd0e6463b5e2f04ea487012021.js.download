    initializeAvailableVariables();
    bindBookingFunctions();
  };

  var bindBookingFunctions = function () {
    $('.career_page').on(
      { mouseenter: bookingHoverStart, mouseleave: bookingHoverStop },
      '.selectable .available'
    );
    $('.career_page').on('click', '.selectable .available', setNewBooking);
    $('.career_page').on('click', '.delete-entry', deleteBooking);
    $(document).on('click', '#bookit', submitBooking);
    $(document).on('click', '#dont_bookit', interruptBooking);
  };

  // Initialize Methods

  var emptyAvailabilitiesNotification = function () {
    if ($('.available').length < 1) {
      $('.empty-availability').show();
