      url: '/career/' + id,
      type: 'DELETE',
      success: function (r) {
        $('#calendar_loading').hide();
        $('.available_' + id).addClass('unselected');
        $('.available_' + id).removeClass(
          'hover_hour available available_' + id
        );
        $("[data-entry='" + id + "']").remove();
        updateCopyDropdown();
      },
      error: function (r) {},
    });
